package week5

object listz {

  def init[T](xs: List[T]): List[T] = xs match {
    case List()  => throw new Error("init of empty list")
    case List(x) => List()
    case y :: ys => y :: init(ys)
  }

  var a = "123456".toList

  init(a)
  
  
  def removeAt[T](n: Int, xs: List[T]) = xs match {
  }
  
}